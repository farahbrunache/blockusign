    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "express-ws": "^4.0.0",
    "linkifyjs": "^2.1.8",
    "lodash": "^4.17.11",
    "next": "^8.0.3",
    "nodemailer": "^5.1.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "path": "^0.12.7",
    "qs": "^6.6.0",
    "radiks-server": "^0.1.7",
    "universal-cookie-express": "^3.0.7"
  },
  "devDependencies": {
