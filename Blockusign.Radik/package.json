  "main": "index.js",
  "scripts": {
    "install": "npm install --save-dev nodemon radiks-server babel-cli babel-node cross-env babel-preset-env webpack html-webpack-plugin webpack-livereload-plugin babel-loader style-loader css-loader babel-core webpack-dev-middleware && npm i",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node index",
    "start": "node index",
    "build": "babel ../Blockusign.Radik --out-dir build && cp .env build",
    "zip": "cd dist && powershell Compress-Archive -Force -Path * -DestinationPath deploy.zip",
    "copy-dist": "cp .env dist && cp package.json dist && cp web.config dist && cp iisnode.yml dist",
    "dist": "webpack && npm run copy-dist"
  },
